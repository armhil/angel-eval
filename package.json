{
  "name": "eval-angel",
  "version": "1.0.4",
  "description": "A conditional expression parser and interpreter.",
  "main": "dist/index.js",
  "repository": {
    "url": "git@github.com:claude-abounegm/eval-angel.git"
  },
  "keywords": [
    "conditional",
    "expression",
    "javascript",
    "nearley",
    "eval",
    "no-eval",
    "module"
  ],
  "scripts": {
    "test": "nyc mocha --require @babel/register",
    "build": "npm run build:grammar && npm run build:app",
    "build:app": "rm -Rf dist && babel src --out-dir dist --copy-files",
    "build:grammar": "nearleyc ./grammar.ne -o ./src/compiledGrammar.js",
    "prepublish": "npm run build"
  },
  "author": "Claude Abounegm <claude.abounegm@gmail.com>",
  "license": "MIT",
  "nyc": {
    "include": "src",
    "exclude": "src/compiledGrammar.js"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "nearley": "^2.20.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/register": "^7.15.3",
    "@types/chai": "^4.2.21",
    "@types/nearley": "^2.11.2",
    "chai": "^4.3.4",
    "coveralls": "^3.1.1",
    "mocha": "^9.1.1",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^15.1.0"
  }
}
